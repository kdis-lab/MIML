<configuration>
	<classifier name="miml.inProgress.MLkNNWrapper">
		<numReferences>5</numReferences>
		<smooth>1.0</smooth>
		<metric name="miml.inProgress.AdaptedAverageHausdorff">
		</metric>
	</classifier>
	
	<evaluator name="miml.evaluation.EvaluatorCV">
		<numFolds>5</numFolds>
		<data>
			<file>data/miml_birds.arff</file>
			<xmlFile>data/miml_birds.xml</xmlFile>
		</data>
	</evaluator>
	
	<report name="miml.report.BaseMIMLReport">
		<header>false</header>
		<fileName>results/prueba.csv</fileName>
		<standardDeviation>false</standardDeviation>
	</report>
	
</configuration>